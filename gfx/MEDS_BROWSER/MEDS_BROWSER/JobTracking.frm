Public Sub DisplayResultsSet()
    Dim i As Integer
    Dim Results As Object
    Dim RowCount As Integer
    
    Call ClearSearchResults
    
    If CurrentQuery = "" Then
        Exit Sub
    End If
    
    Set Results = OraDatabase.CreateDynaset(CurrentQuery, 0&)
    
    RowCount = 0
    i = 0
    
    While Not Results.EOF And i < PONumber.Count
        
        If RowCount >= VScroll.value Then
            MEIC_Number(i).Text = Results("meic_number")
            MEIC_Number(i).Visible = True
            If Results("po_number") <> "" Then
                PONumber(i).Text = Results("po_number")
                If Results("po_task") <> "" Then
                    PONumber(i).Text = Results("po_number") & Results("po_task")
                End If
            End If
            PONumber(i).Visible = True
            Status(i).Text = Results("status")
            Status(i).Visible = True
            If Results("supplier") <> "" Then
                Supplier(i).Text = Results("supplier")
            End If
            Supplier(i).Visible = True
            JobType(i).Text = Results("description")
            JobType(i).Visible = True
            If Results("assigned") <> "" Then
                Assigned(i).Text = Results("assigned")
            End If
            Assigned(i).Visible = True
            If Results("qa_processing_target") <> "" Then
                QaTarget(i).Text = Results("qa_processing_target")
            End If
            QaTarget(i).Visible = True
            If Results("date_received") <> "" Then
                DateReceived(i).Text = Results("date_received")
            End If
            DateReceived(i).Visible = True
            If Results("source") <> "" Then
                Source(i).Text = Results("source")
            End If
            Source(i).Visible = True
            If Results("classification") <> "" Then
                Classification(i).Text = Results("classification")
            End If
            Classification(i).Visible = True
            Edit(i).Visible = True
            MEIC_Number(i).Text = Results("MEIC_Number")
            i = i + 1
        End If
        Results.MoveNext
        RowCount = RowCount + 1
    Wend
    
    If NumberOfResults > 0 Then
        ResultsFrame.Caption = VScroll.value + 1 & " to " & VScroll.value + i & " of " & NumberOfResults & " in Job_Tracking"
    Else
        ResultsFrame.Caption = "No Records Found"
    End If
    
    If NumberOfResults < PONumber.UBound Then
        VScroll.Visible = False
    Else
        VScroll.Visible = True
    End If

End Sub


