Public Sub GetFileLocation(file_name As String, _
                            ByRef path_name As String, _
                            ByRef sheet_name As String)
    Dim Results As Object
    Dim SQLQuery As String
    
    path_name = ""
    
    SQLQuery = "SELECT path_name, argument FROM file_lookup where file_name = '" & file_name & "'"
    Set Results = OraDatabase.CreateDynaset(SQLQuery, 0&)
    
    If Not Results.EOF Then
        path_name = Results("path_name")
        sheet_name = Results("argument")
    End If

End Sub

Private Function AlreadyLoaded(ByRef oExcel As Object, _
                            workbook_name As String) As String
    Dim i As Integer
    AlreadyLoaded = False
    For i = 1 To oExcel.Workbooks.Count
        If workbook_name = oExcel.Workbooks(i).Name Then
            AlreadyLoaded = True
        End If
    Next
    
End Function


Public Sub ViewInExcel(file_name As String)
    Dim oExcel As Object
    Dim worksheet As Object
    Dim spreadsheet As String
    Dim sheet_name As String
    Dim RefreshCommand As Object
    
    On Error GoTo OpenExcel
    
    Call GetFileLocation(file_name, spreadsheet, sheet_name)
    
    If spreadsheet <> "" Then
    
        Set oExcel = GetObject(, "Excel.Application")

        oExcel.Visible = True
        
        If AlreadyLoaded(oExcel, file_name) = False Then
            ' there is an excel running but the spreadsheet is not loaded
            oExcel.Workbooks.Open spreadsheet
        End If
        
        oExcel.GoTo (oExcel.Sheets(sheet_name).Cells(1, 1))
    
        Set RefreshCommand = oExcel.CommandBars("External Data").Controls("&Refresh Data")
 
        RefreshCommand.Execute
    
        oExcel.Visible = True
    
        'Set worksheet = oExcel.Workbooks(1)
    Else
        MsgBox "Error Looking up '" & file_name & "' in table file_lookup"
        
    End If
    
    Exit Sub
    
OpenExcel:
    On Error GoTo ErrorOpening
    
    Set oExcel = CreateObject("Excel.Application")
    
    oExcel.Visible = True
    
    oExcel.Workbooks.Open spreadsheet
    
    oExcel.GoTo (oExcel.Sheets(sheet_name).Cells(1, 1))
    
    Set RefreshCommand = oExcel.CommandBars("External Data").Controls("&Refresh Data")
 
    RefreshCommand.Execute
    
    Exit Sub
    
ErrorOpening:
    MsgBox "Error opening spreadsheet '" & spreadsheet & "'"
    
End Sub

