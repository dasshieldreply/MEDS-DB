Private Sub SearchJobs_Click()
    Dim SQLQuery As String
    Dim WhereQuery As String
    Dim Results As Object
    Dim i As Integer

    
    WhereQuery = "FROM job_tracking j, meds_job_type t " & _
                  "WHERE j.job_type_number = t.job_type_number "

    If SearchMEICNumber.Text <> "" Then
        WhereQuery = WhereQuery & " AND meic_number = '" & SearchMEICNumber.Text & "'"
    End If
    
    If SearchPONumber.Text <> "" Then
        WhereQuery = WhereQuery & " AND po_number = '" & SearchPONumber.Text & "'"
    End If
    
    If SearchStatus.Text <> "" Then
        WhereQuery = WhereQuery & " AND status = '" & SearchStatus.Text & "'"
    End If
    
    If SearchSupplier.Text <> "" Then
        WhereQuery = WhereQuery & " AND supplier like '%" & SearchSupplier.Text & "%'"
    End If
    
    If SearchJobType.Text <> "" Then
        WhereQuery = WhereQuery & " AND t.description = '" & SearchJobType.Text & "'"
    End If
    
    If SearchAssigned.Text <> "" Then
        WhereQuery = WhereQuery & " AND assigned = '" & SearchAssigned.Text & "'"
    End If
       
    If SearchSource.Text <> "" Then
        WhereQuery = WhereQuery & " AND source = '" & SearchSource.Text & "'"
    End If
    
    If SearchClassification.Text <> "" Then
        WhereQuery = WhereQuery & " AND classification = '" & SearchClassification.Text & "'"
    End If
    
    If SearchFrom <> "" Then
        WhereQuery = WhereQuery & " AND date_received >= TO_DATE('" & _
            SearchFrom.Day & "/" & SearchFrom.Month & "/" & SearchFrom.Year & "','DD/MM/YYYY') "
    End If
    
    If SearchTo <> "" Then
        WhereQuery = WhereQuery & " AND date_received <= TO_DATE('" & _
            SearchTo.Day & "/" & SearchTo.Month & "/" & SearchTo.Year & "','DD/MM/YYYY') "
    End If
    
    If QATargetFrom <> "" Then
        WhereQuery = WhereQuery & " AND QA_PROCESSING_TARGET >= TO_DATE('" & _
            QATargetFrom.Day & "/" & QATargetFrom.Month & "/" & QATargetFrom.Year & "','DD/MM/YYYY') "
    End If
    
    If QATargetTo <> "" Then
        WhereQuery = WhereQuery & " AND QA_PROCESSING_TARGET <= TO_DATE('" & _
            QATargetTo.Day & "/" & QATargetTo.Month & "/" & QATargetTo.Year & "','DD/MM/YYYY') "
    End If
   
    SQLQuery = "Select count(*) " & WhereQuery
    Set Results = OraDatabase.CreateDynaset(SQLQuery, 0&)
        
    NumberOfResults = Results("count(*)")
    
    SQLQuery = "SELECT meic_number, po_number, po_task, supplier, status, description, assigned, " & _
                "      acknowledge, date_received, source, classification, qa_processing_target " & _
                WhereQuery & " ORDER BY " & OrderBy.Text & " " & OrderDirection.Text
    
    CurrentQuery = SQLQuery
    
    VScroll.Min = 0
    VScroll.Max = NumberOfResults - PONumber.UBound
    VScroll.value = 0
    VScroll.SmallChange = 10
    VScroll.LargeChange = 20
    
    Call DisplayResultsSet

End Sub



    
Public Sub ForceFloatText(ByRef box As Object)
    Dim i As Integer
    Dim filtered As String
    Dim state As StateFloatChecking

    state = STATE_CHECKING_SIGN
    
    'check for sign
    i = 1
    filtered = ""
    While i <= Len(box.Text)
        Dim character As String
        
        character = Mid(box.Text, i, 1)
        If state = STATE_CHECKING_SIGN Then
            If character = "+" Or character = "-" Then
                filtered = filtered & character
                i = i + 1
            End If
            state = STATE_CHECKING_DECIMAL_PART
        End If
        
        character = Mid(box.Text, i, 1)
        If state = STATE_CHECKING_DECIMAL_PART Then
            If (character >= "0" And character <= "9") Or character = "." Then
                filtered = filtered & character
                If character = "." Then
                    state = STATE_CHECKING_FRACTION_PART
                End If
            End If

            i = i + 1
        End If
        
        character = Mid(box.Text, i, 1)
        If state = STATE_CHECKING_FRACTION_PART Then
            If (character >= "0" And character <= "9") Then
                filtered = filtered & character
            End If

            i = i + 1
        End If
    Wend
    
    If (filtered <> box.Text) Then
        box.Text = filtered
    End If
End Sub

Public Sub ForceIntegerText(ByRef box As Object)
    Dim i As Integer
    Dim filtered As String
    
    filtered = ""
    For i = 1 To Len(box.Text)
        Dim character As String
        character = Mid(box.Text, i, 1)
        If character >= "0" And character <= "9" Then
            filtered = filtered & character
        End If
    Next i
    If (filtered <> box.Text) Then
        box.Text = filtered
    End If
End Sub

